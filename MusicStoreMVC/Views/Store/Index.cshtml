@model MusicStoreMVC.ViewModels.StoreIndexViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/SiteMaster.cshtml";
}

<h2>Store Home</h2>
<p>Select from @Model.NumberofGenres genres:</p>
<ul>
    @foreach (string genreName in @Model.Genres)
    {
        <li>
            @genreName
        </li>
    }
</ul>
